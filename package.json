{
  "name": "ember-cli-browserstack",
  "version": "1.1.0",
  "description": "Cross-browser testing with BrowserStack",
  "keywords": [
    "ember-addon",
    "browserstack"
  ],
  "repository": "kategengler/ember-cli-browserstack",
  "license": "MIT",
  "author": "Katie Gengler",
  "bin": {
    "browserstack-launch": "lib/tasks/launch-browserstack-browser.js"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember:browserstack": "ember test --test-port=7774 --host=127.0.0.1 --config-file=testem.browserstack.js",
    "test:browserstack": "npm-run-all browserstack:connect test:ember:browserstack browserstack:disconnect browserstack:results",
    "test:ember-compatibility": "ember try:each",
    "test:node": "mocha node-tests/**/*-test.js",
    "browserstack:connect": "ember browserstack:connect",
    "browserstack:disconnect": "ember browserstack:disconnect",
    "browserstack:results": "ember browserstack:results"
  },
  "dependencies": {
    "browserstack": "^1.6.1",
    "browserstack-local": "^1.4.8",
    "rsvp": "^4.8.5",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.41.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.3.4",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.27.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.27.2",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.4.2",
    "ember-try": "^1.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.4.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-qunit": "^6.1.1",
    "execa": "^5.1.1",
    "lerna-changelog": "^2.1.0",
    "loader.js": "^4.7.0",
    "mocha": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "changelog": {
    "repo": "kategengler/ember-cli-browserstack",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  }
}
